{
  "id": null,
  "uid": "sbrl-api",
  "title": "Spring Boot API Overview",
  "tags": ["spring-boot", "micrometer", "prometheus"],
  "timezone": "browser",
  "schemaVersion": 39,
  "version": 1,
  "refresh": "5s",
  "time": {
    "from": "now-15m",
    "to": "now"
  },
  "templating": {
    "list": [
      {
        "type": "datasource",
        "name": "DS_PROMETHEUS",
        "label": "Prometheus",
        "query": "prometheus",
        "current": {
          "text": "Prometheus",
          "value": "Prometheus"
        }
      },
      {
        "type": "query",
        "name": "uri",
        "label": "URI",
        "datasource": {
          "type": "prometheus",
          "uid": "${DS_PROMETHEUS}"
        },
        "refresh": 2,
        "hide": 0,
        "includeAll": true,
        "multi": true,
        "allValue": ".*",
        "query": {
          "query": "label_values(http_server_requests_seconds_count, uri)",
          "refId": "PromQuery"
        },
        "current": {
          "text": "All",
          "value": ".*"
        }
      }
    ]
  },
  "panels": [
    {
      "type": "stat",
      "title": "RPS (all)",
      "gridPos": {"x": 0, "y": 0, "w": 6, "h": 4},
      "datasource": {"type": "prometheus", "uid": "${DS_PROMETHEUS}"},
      "targets": [
        {
          "refId": "A",
          "expr": "sum(rate(http_server_requests_seconds_count[1m]))"
        }
      ],
      "options": {"reduceOptions": {"calcs": ["lastNotNull"], "fields": "", "values": false}}
    },
    {
      "type": "stat",
      "title": "5xx RPS (all)",
      "gridPos": {"x": 6, "y": 0, "w": 6, "h": 4},
      "datasource": {"type": "prometheus", "uid": "${DS_PROMETHEUS}"},
      "targets": [
        {
          "refId": "A",
          "expr": "sum(rate(http_server_requests_seconds_count{status=~\"5..\"}[1m]))"
        }
      ],
      "options": {"reduceOptions": {"calcs": ["lastNotNull"], "fields": "", "values": false}}
    },
    {
      "type": "stat",
      "title": "Tomcat threads busy",
      "gridPos": {"x": 12, "y": 0, "w": 6, "h": 4},
      "datasource": {"type": "prometheus", "uid": "${DS_PROMETHEUS}"},
      "targets": [
        {
          "refId": "A",
          "expr": "tomcat_threads_busy_threads"
        }
      ],
      "options": {"reduceOptions": {"calcs": ["lastNotNull"], "fields": "", "values": false}}
    },
    {
      "type": "stat",
      "title": "In-flight requests (custom)",
      "gridPos": {"x": 18, "y": 0, "w": 6, "h": 4},
      "datasource": {"type": "prometheus", "uid": "${DS_PROMETHEUS}"},
      "targets": [
        {
          "refId": "A",
          "expr": "in_flight"
        }
      ],
      "options": {"reduceOptions": {"calcs": ["lastNotNull"], "fields": "", "values": false}},
      "description": "This panel will show data only if you also export a Prometheus metric named 'in_flight'."
    },
    {
      "type": "timeseries",
      "title": "RPS by URI",
      "gridPos": {"x": 0, "y": 4, "w": 12, "h": 8},
      "datasource": {"type": "prometheus", "uid": "${DS_PROMETHEUS}"},
      "targets": [
        {
          "refId": "A",
          "expr": "sum by (uri) (rate(http_server_requests_seconds_count{uri=~\"$uri\"}[1m]))",
          "legendFormat": "{{uri}}"
        }
      ]
    },
    {
      "type": "timeseries",
      "title": "5xx RPS by URI",
      "gridPos": {"x": 12, "y": 4, "w": 12, "h": 8},
      "datasource": {"type": "prometheus", "uid": "${DS_PROMETHEUS}"},
      "targets": [
        {
          "refId": "A",
          "expr": "sum by (uri) (rate(http_server_requests_seconds_count{status=~\"5..\",uri=~\"$uri\"}[1m]))",
          "legendFormat": "{{uri}}"
        }
      ]
    },
    {
      "type": "timeseries",
      "title": "Average latency (seconds) by URI",
      "gridPos": {"x": 0, "y": 12, "w": 12, "h": 8},
      "datasource": {"type": "prometheus", "uid": "${DS_PROMETHEUS}"},
      "targets": [
        {
          "refId": "A",
          "expr": "sum by (uri) (rate(http_server_requests_seconds_sum{uri=~\"$uri\"}[5m])) / sum by (uri) (rate(http_server_requests_seconds_count{uri=~\"$uri\"}[5m]))",
          "legendFormat": "{{uri}}"
        }
      ]
    },
    {
      "type": "timeseries",
      "title": "p95 latency (seconds) by URI (requires histogram)",
      "gridPos": {"x": 12, "y": 12, "w": 12, "h": 8},
      "datasource": {"type": "prometheus", "uid": "${DS_PROMETHEUS}"},
      "targets": [
        {
          "refId": "A",
          "expr": "histogram_quantile(0.95, sum by (le, uri) (rate(http_server_requests_seconds_bucket{uri=~\"$uri\"}[5m])))",
          "legendFormat": "{{uri}}"
        }
      ]
    },
    {
      "type": "timeseries",
      "title": "Tomcat threads busy/current",
      "gridPos": {"x": 0, "y": 20, "w": 24, "h": 7},
      "datasource": {"type": "prometheus", "uid": "${DS_PROMETHEUS}"},
      "targets": [
        {"refId": "A", "expr": "tomcat_threads_busy_threads", "legendFormat": "busy"},
        {"refId": "B", "expr": "tomcat_threads_current_threads", "legendFormat": "current"}
      ]
    },
    {
      "type": "timeseries",
      "title": "In-flight requests (custom)",
      "gridPos": {"x": 0, "y": 27, "w": 24, "h": 7},
      "datasource": {"type": "prometheus", "uid": "${DS_PROMETHEUS}"},
      "targets": [
        {
          "refId": "A",
          "expr": "in_flight",
          "legendFormat": "in_flight"
        }
      ]
    }
  ]
}
